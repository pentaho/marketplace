        <class_name>Processor</class_name>

        <class_source><![CDATA[
import java.io.InputStream;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.text.ParseException;

import com.sun.jersey.core.header.FormDataContentDisposition;
import org.apache.commons.io.FilenameUtils;

import org.pentaho.platform.web.http.api.resources.RepositoryImportResource;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException { 

	// First, get a row from the default input hop
	//
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}
	logBasic("importing");

	

	//variables
	String sampleZipFullPath = getParameter("DownloadDestination");

	logBasic("Sample zip is at: " + sampleZipFullPath);
	InputStream input = null;
	try {
		input = new FileInputStream(sampleZipFullPath);
	} catch(FileNotFoundException ex) {
		logBasic("Inputstream: " + ex.getMessage());
	}
	
	 
	String zipFileName = FilenameUtils.getName(sampleZipFullPath);

	logBasic("Zip file name is " + zipFileName);

	FormDataContentDisposition fileInfo = null;
	try {
		fileInfo = new FormDataContentDisposition("form-data; name=\"" + zipFileName + "\"; filename=\"" + zipFileName + "\"");
	} catch(ParseException ex){
		logBasic("fileInfo: " + ex.getMessage());
	}

	logBasic("Ready to start importing samples");
	RepositoryImportResource repositoryImporter = new RepositoryImportResource();
	repositoryImporter.doPostImport(getParameter("SamplesDir"), input, "true", "true", "true", "true", "UTF-8", "DEBUG", fileInfo);
	
	logBasic("Finished importing samples");
	return true;
}]]></class_source>
